! Constant C2_SWITCH;   ! Latin 2 (Central/Eastern Europe)
! Constant C3_SWITCH;   ! Latin 3 (Southern Europe)
! Constant C4_SWITCH;   ! Latin 4 (Northern Europe / Baltic)
  Constant C5_SWITCH;   ! Cyrillic
! Constant C6_SWITCH;   ! Arabic
! Constant C7_SWITCH;   ! Greek
! Constant C8_SWITCH;   ! Hebrew
! Constant C9_SWITCH;   ! Latin 5 (Turkish)

[ Unicode c exist;
    if ($0032->0 < 1) { @print_char '#'; return; }
    @check_unicode c -> exist;
    if (exist & $0001) @print_unicode c;
    else               @print_char '#';
];

#Ifdef C1_SWITCH;

[ U c;
    switch (c) {
      'a' to 'z':
        c = c - 32;
      201, 203, 211, 213, 220:
        c++;
      215, 216:
        c = c + 2;
      155 to 157, 164 to 165, 205 to 207: ! was 164 to 166
        c = c + 3;
      181 to 185, 191 to 195:
        c = c + 5 ;
      169 to 174:
        c = c + 6;
    }
    print (char) c;
];

[ L c;
    switch (c) {
      'A' to 'Z':
        c = c + 32;
      202, 204, 212, 214, 221:
        c--;
      217, 218:
        c = c - 2;
      158 to 160, 167 to 168, 208 to 210: ! was 167 to 169
        c = c - 3;
      186 to 190, 196 to 200:
        c = c - 5 ;
      175 to 180:
        c = c - 6;
    }
    print (char) c;
];

